# vim: syntax=python

rule fig3_umap_plot:
    input:
        "input/any___sc{umap}seq___none.csv"
    output:
        "fig3/sc{umap}seq/umap_plot.svg",
        "fig3/sc{umap}seq/umap_plot.pdf",
        "fig3/sc{umap}seq/umap_plot.jpeg"
    resources:
        mem=24000
    log:
        "fig3/sc{umap}seq/umap_plot.log"
    script:
        "scripts/plot_umap.R"
