# vim: syntax=python

module scan2_rescue_placeholder_changeme:
    snakefile: "snakefile.scan2"
    config: config

use rule scan2_rescue_run from scan2_rescue_placeholder_changeme as scan2_rescue_run_placeholder_changeme with:
    output:
        rdas=protected(expand('scan2/rescue_{{rescue_group}}/objects/{sample}_scan2_object_rescue.rda',
            sample=config['scan2_rescue_groups']['placeholder_changeme'].values()))
